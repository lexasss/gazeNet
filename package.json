{
  "name": "gazeNet",
  "version": "0.0.1",
  "description": "Shares gaze data over the net",
  "author": "Terlici Ltd.",
  "license": "MIT",
  "main": "/.bin/www",
  "config": {
    "web": {
      "host": "localhost",
      "port": 88
    },
    "front": {
      "title": "GazeNet"
    },
    "back": {},
    "db": {
      "host": "localhost",
      "port": 27017,
      "name": "gazenet"
    }
  },
  "scripts": {
    "start": "grunt && db && (start grunt watch & node ./bin/www)"
  },
  "browserify": {
    "transform": [ "browserify-shim" ]
  },
  "browser": {
    "md5": "./ui/libs/md5.js"
  },
  "browserify-shim": {
    "md5": "md5"
  },
  "dependencies": {
    "express": "~4.8.6",
    "body-parser": "~1.6.6",
    "cookie-parser": "~1.3.2",
    "morgan": "~1.2.3",
    "serve-favicon": "~2.0.1",
    "debug": "~1.0.4",
    "jade": "~1.11.0",
    "ws": "~0.4.32",
    "mongodb": "~2.0.33",
    "monk": "~1.0.1",
    "browserify": "~11.0.1",
    "browserify-shim": "~3.8.10"
  },
  "devDependencies": {
    "grunt-contrib-less": "latest",
    "grunt-contrib-watch": "latest",
    "grunt-contrib-connect": "~0.9.0",
    "grunt-contrib-concat": "~0.5.1",
    "grunt-contrib-clean": "~0.6.0",
    "grunt-contrib-copy": "~0.8.0",
    "grunt-autoprefixer": "~3.0.3",
    "grunt-browserify": "~4.0.0"
  }
}
